    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2e19eb13-fc47-4e35-a3ca-62e6625a845c"
      }
    },
    "android": {
      "package": "com.shalom2025.Recipe_App"
    }
  }
}
